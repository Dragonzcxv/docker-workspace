[mysqld]
skip-log-bin

# Off strict mode
sql_mode=""

log-error   = /var/log/mysql/error.log
# general_log_file = /var/log/mysql/log_output.log
# general_log      = 1

# Slow query settings:
slow_query_log=0
slow_query_log_file=/var/log/mysql/slow.log
long_query_time=10
innodb_strict_mode=OFF
